<% include header %>
<div class="container">
  <div class="row">
    <div class="col-md-2">
      <ul class="list-group">
        <li class="list-group-item active">
          <a href="#">西祠代理</a>
        </li>
        <li class="list-group-item">
          <a href="#">今日代理</a>
        </li>
        <li class="list-group-item">
          <a href="#">西祠代理</a>
        </li>
      </ul>
    </div>
    <div class="col-md-10">
      <div class="panel panel-default">
        <div class="panel-heading">
          <p><%= title %></p>
        </div>
        <ul class="nav nav-tabs">
          <li class=""><a href="#">国内代理</a></li>
          <li class="active"><a href="#">国外代理</a></li>
        </ul>
        <div class="panel-body">
          <table class="table table-hover table-striped">
            <thead>
            <th>编号</th>
            <th>ip</th>
            <th>端口</th>
            <th>地域</th>
            <th>匿名</th>
            <th>类型</th>
            <th>时间</th>
            </thead>
            <tbody>
            <% for(var i = 0; i < ips.length; i++) { %>
            <tr>
              <td><%=ips[i].id%></td>
              <td><%=ips[i].ip%></td>
              <td><%=ips[i].port%></td>
              <td><%=ips[i].dis%></td>
              <td><%=ips[i].user%></td>
              <td><%=ips[i].type%></td>
              <td><%=ips[i].time%></td>
            </tr>
            <% } %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
<script>
  $(document).ready(function () {
    $('.list-group li').click(function () {
      $('.list-group li').removeClass('active');
      $(this).addClass('active');
    });
    $('ul.nav.nav-tabs li').click(function () {
      $('ul.nav.nav-tabs li').removeClass('active');
      $(this).addClass('active');
    })
  })
</script>
<% include footer %>
